{
  "homepage": "https://github.com/domicch/life-in-uk",
  "name": "life-in-uk-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:github": "NEXT_PUBLIC_BUILD_TIME=$(date -u +%Y-%m-%dT%H:%M:%S) GITHUB_PAGES=true NODE_ENV=production next build",
    "build:local": "NEXT_PUBLIC_BUILD_TIME=$(date -u +%Y-%m-%dT%H:%M:%S) NODE_ENV=production next build",
    "start": "npx serve@latest out -p 3000",
    "start:prod": "npm run build:local && npx serve@latest out -p 3000",
    "start:github": "npm run build:github && npx serve@latest out -p 3000",
    "lint": "next lint",
    "predeploy": "npm run build:github",
    "deploy": "gh-pages -d out"
  },
  "dependencies": {
    "next": "14.0.4",
    "papaparse": "^5.4.1",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/papaparse": "^5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "gh-pages": "^6.3.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
